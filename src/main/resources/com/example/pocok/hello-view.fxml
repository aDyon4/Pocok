<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pocok.HelloController">
   <children>
      <Pane fx:id="pnJatek" prefHeight="788.0" prefWidth="788.0" style="-fx-background-color: #b9d753; -fx-border-color: black;">
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </Pane>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label style="-fx-border-color: black;">
               <graphic>
                  <ImageView fitHeight="128.0" fitWidth="128.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@pocok1.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Label>
            <Label text="0">
               <font>
                  <Font name="System Bold" size="64.0" />
               </font>
            </Label>
            <Label text="0/0">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label text="0%">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Stop" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
